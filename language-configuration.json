{
	"comments": {
		"lineComment": "//",
		"blockComment": [ "\\{", "\\}" ]
	},
	"brackets": [
		["#(", ")"],
		["#[", "]"],
		["(", ")"],
		["[", "]"],
		["{", "}"]
	],
	"autoClosingPairs": [
		["#(", ")"],
		["#[", "]"],
		["(", ")"],
		["[", "]"],
		["{", "}"],
		["'", "'"]
	],
	"surroundingPairs": [
		["#(", ")"],
		["#[", "]"],
		["(", ")"],
		["[", "]"],
		["{", "}"],
		["'", "'"]
	],
	"wordPattern": "&?[a-zA-Z_]\\w*"
}
