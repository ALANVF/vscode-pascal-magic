{
	"name": "language-pascal",
	"displayName": "Pascal magic",
	"description": "High-quality Pascal highlighting",
	"version": "0.1.6",
	"publisher": "theangryepicbanana",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ALANVF/vscode-pascal-magic.git"
	},
	"engines": {
		"vscode": "^1.47.3"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"keywords": [
		"pascal",
		"free pascal",
		"object pascal",
		"delphi",
		"fpc",
		"objfpc"
	],
	"activationEvents": [
		"onLanguage:pascal"
	],
	"main": "./out/extension",
	"contributes": {
		"languages": [
			{
				"id": "pascal",
				"aliases": [
					"Pascal",
					"Object Pascal",
					"Free Pascal",
					"FPC",
					"Delphi",
					"pascal",
					"object pascal",
					"free pascal",
					"fpc",
					"delphi"
				],
				"extensions": [
					".pas",
					".pp",
					".dfm",
					".dpr",
					".dpk",
					".lfm",
					".lpr"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"old-grammars": [
			{
				"language": "pascal",
				"scopeName": "source.pascal",
				"path": "./syntaxes/pascal.json"
			}
		],
		"snippets": [
			{
				"language": "pascal",
				"path": "./snippets/pascal.json"
			}
		],
		"semanticTokenScopes": [
			{
				"language": "pascal",
				"scopes": {
					"keyword-operator": [
						"keyword.operator.pascal"
					],
					"constant": [
						"constant.language.pascal"
					],
					"label": [
						"variable.label.pascal"
					],
					"invalid": [
						"invalid.illegal.pascal"
					]
				}
			}
		]
	},
	"scripts": {
		"compile-syntax": "reon-convert tj syntaxes/pascal.reon",
		"compile-snippets": "reon-convert tj snippets/pascal.reon",
		"compile-ts": "tsc -p ."
	},
	"dependencies": {
		"@types/vscode": "^1.52.0",
		"onigasm": "^2.2.5",
		"vscode-oniguruma": "^1.4.0",
		"vscode-textmate": "^5.2.0"
	},
	"devDependencies": {
		"@types/node": "12.12.2",
		"reon-convert": "^0.1.13",
		"typescript": "^4.3.5"
	}
}