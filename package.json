{
	"name": "language-pascal",
	"displayName": "Pascal magic",
	"description": "High-quality Pascal highlighting",
	"version": "0.1.1",
	"publisher": "theangryepicbanana",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ALANVF/vscode-pascal-magic.git"
	},
	"engines": {
		"vscode": "^1.47.3"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"keywords": [
		"pascal",
		"free pascal",
		"object pascal",
		"delphi",
		"fpc",
		"objfpc"
	],
	"contributes": {
		"languages": [
			{
				"id": "pascal",
				"aliases": [
					"Pascal",
					"Object Pascal",
					"Free Pascal",
					"FPC",
					"Delphi",
					"pascal",
					"object pascal",
					"free pascal",
					"fpc",
					"delphi"
				],
				"extensions": [
					".pas",
					".pp",
					".dfm",
					".dpr",
					".dpk",
					".lfm",
					".lpr"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "pascal",
				"scopeName": "source.pascal",
				"path": "./syntaxes/pascal.json"
			}
		],
		"snippets": [
			{
				"language": "pascal",
				"path": "./snippets/pascal.json"
			}
		]
	},
	"scripts": {
		"compile-syntax": "reon-convert tj syntaxes/pascal.reon",
		"compile-snippets": "reon-convert tj snippets/pascal.reon"
	},
	"devDependencies": {
		"reon-convert": "^0.1.3"
	}
}
